@font-face
  font-family 'Game of Thrones'
  src url('font/GameOfThrones.ttf')

 h1
    font-family Game of Thrones
    color white

p
    color white


body
    background-image url("img/game-of-thrones-background.jpg")

#holder
  height  350px
  width  850px
  border-radius 5px
  // background-color  #F5F5F5
  background-color  #2c2c2c
  border  2px solid #A4A4A4
  margin  0 auto

#place
  position  relative
  margin  20px

#place a
  font-size  10px
  padding 7px
  margin-bottom 30px
  margin-left 15px
  text-align right

#place li
  list-style  none outside none
  position  absolute              //keeps seats in row if removed makes one long column
  text-align right

  
  

#place li hover
  background-color  yellow

#place .seat
  background-image  url("img/seat.png")
  background-size  35px
  background-repeat  no-repeat
  background-position 0% 100%  no-repeat scroll 0 0 transparent
  height  55px
  width  50px
  display  block


/*Booked Seats */
#place .selectedSeat
  background-image  url("img/booked.png")
  background-repeat  no-repeat
  background-size  45px
  background-position  left


#place .selectingSeat
  background-image  url("img/got.png")
  background-repeat  no-repeat
  background-size  40px
  background-position  top left 
  

#place .row-3,
#place .row-4
  margin-top  40px


#seatDescription li
  /*verticle-align  middle */
  list-style  none outside none
  padding-left  35px
  height  100px
  width  150px
  display inline
  float  center
  color white

#key
  padding 20px
  width 100%

ul 
  padding 10px


img
/*  height  50px
  width  50px */
  margin  5px

form
    width 100%
    margin auto
    align center
    display block
    text-align center
    padding 10px
    
label
    color white
    margin-right 10px

.container
    text-align center
    color 
